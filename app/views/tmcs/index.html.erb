<p id="notice"><%= notice %></p>

<h1>Tmcs</h1>

<table>
  <thead>
    <tr>
      <th>Valor</th>
      <th>Fecha</th>
      <th>Titulo</th>
      <th>Subtitulo</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tmcs.each do |tmc| %>
      <tr>
        <td><%= tmc.valor %></td>
        <td><%= tmc.fecha %></td>
        <td><%= tmc.titulo %></td>
        <td><%= tmc.subtitulo %></td>
        <td><%= tmc.tipo %></td>
        <td><%= link_to 'Show', tmc %></td>
        <td><%= link_to 'Edit', edit_tmc_path(tmc) %></td>
        <td><%= link_to 'Destroy', tmc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tmc', new_tmc_path %>
