<p id="notice"><%= notice %></p>

<h1>Ufs</h1>

<table>
  <thead>
    <tr>
      <th>Valor</th>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ufs.each do |uf| %>
      <tr>
        <td><%= uf.valor %></td>
        <td><%= uf.fecha %></td>
        <td><%= link_to 'Show', uf %></td>
        <td><%= link_to 'Edit', edit_uf_path(uf) %></td>
        <td><%= link_to 'Destroy', uf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Uf', new_uf_path %>
